# Online Python compiler (interpreter) to run Python online.
# Write Python 3 code in this online editor and run it.
runOrNot = input('Please to start Y and to stop N ')
while runOrNot.lower() == 'y' :
    list10 = input('Please give us a string ')
    x = input('How many times you want to itterate ')
    list20 = [*list10]
    outputList = []

    # for y in list20:
    #     print(y)
    #     for i in range(int(x)) :
    #         print(i + 1)

    for i in range(int(x)):
        for y in list20:
            # print(y)
            a = i + 1
            outputList.append(y + str(a))
    print(outputList)
    runOrNot = input('Please to start Y and to stop N ') 

